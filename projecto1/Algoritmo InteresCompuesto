Algoritmo InteresCompuesto
{ //Variables
  Enteros t=349, P=24 ;
  Flotante  IC, r=0.06 ;
  
  // Imprimir ("Introduzca cuanto dinero se invirtio.")  ;
  // Leer P;
  // Imprimir ("Introduzca cuantos a√±os tomo." ;
  // Leer t;
  
  IC = (P*(1+r)**t);
  
  Imprimir ("Su interes compuesto es:", IC);

} //Fin Algoritmo
