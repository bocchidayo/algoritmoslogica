Algoritmo Fibonacci
{ //Definir variables
  Entero en, n1, n2, sn, f, contador;
  Imprimir {"Ingrese el numero con el que desea iniciar:”);
  leer (f);
  Imprimir (" introduzca la cantidad de numeras que desea mostrar:");
  Leer (cn);
  Si (cn<= 0)entonces
  { Imprimir (“la cantidad de números debe ser mayor a 0”);
  }Sino 
    { n1=0
      n2=f
      Imprimir (“Los números de la serie Fibonacci son”);
      contador = -1
      Mientras (contador <= cn)
       {
        escribir (n1);
        sn=n1+n2
        n1=n2
        n2=sn
        contador= contador + 1
        } //fin Mientras
     } //fin sino
} //fin Algoritmo
